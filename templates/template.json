{
  "background": "assets/background.png",
  "elements": [
    {
      "type": "image",
      "name": "avatar",
      "x": 250,
      "y": 260,
      "width": 400,
      "height": 400,
      "clip": "circle",
      "border": {
        "width": 20,
        "color": "{{ border_color }}"
      },
      "source": "{{ avatar_url }}"
    },
    {
      "type": "text",
      "name": "sales_name",
      "text": "{{ sales_name }}",
      "x": 450,
      "y": 720,
      "font": "DB-Adman-X",
      "fontSize": 168,
      "color": "#000000",
      "align": "center"
    },
    {
      "type": "rectangle",
      "name": "rank_badge",
      "x": 200,
      "y": 780,
      "width": 500,
      "height": 60,
      "radius": 10,
      "color": "{{ badge_color }}"
    },
    {
      "type": "text",
      "name": "rank_name",
      "text": "{{ rank_name }}",
      "x": 450,
      "y": 810,
      "font": "DB-Adman-X",
      "fontSize": 28,
      "color": "#FFFFFF",
      "align": "center"
    }
  ]
}